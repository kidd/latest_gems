<table>
  <tr>
    <th>Name</th>
    <th>info</th>
    <th>Version</th>
    <th>Author</th>
  </tr>

  <%= @gems.first.inspect %>

  <% @gems.each do |gem| %>
    <tr>
      <td> <%= gem["name"] %> </td>
      <td> <%= gem["info"] %> </td>
      <td> <%= gem["version"] %> </td>
      <td> <%= gem["authors"] %> </td>
    </tr>
  <% end %>
</table>
